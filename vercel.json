{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "outputDirectory": "build"
}
